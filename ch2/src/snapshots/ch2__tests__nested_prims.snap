extern rt_print_int, rt_print_newline, rt_read_int
section .text

.start:
    call rt_read_int
    mov qword [rbp - 8], rax
    mov qword [rbp - 16], 5
    call rt_read_int
    mov qword [rbp - 24], rax
    mov rax, qword [rbp - 24]
    mov qword [rbp - 32], rax
    neg qword [rbp - 32]
    mov qword [rbp - 40], -2
    mov rax, qword [rbp - 16]
    add qword [rbp - 40], rax
    mov rax, qword [rbp - 32]
    add rax, qword [rbp - 40]
    jmp .conclusion

    global _start
_start:
    push rbp
    mov rbp, rsp
    sub rsp, 48
