extern read_int, print_int, print_newline
section .text

start:
    mov rbx, 1
    mov qword [rbp - 8], 42
    add rbx, 7
    mov rcx, rbx
    add rcx, qword [rbp - 8]
    neg rbx
    mov rax, rcx
    add rax, rbx
    jmp conclusion

    global _start
_start:
    push rbp
    mov rbp, rsp
    sub rsp, 8
    push rbx
    jmp start

conclusion:
    pop rbx
    call print_int
    call print_newline
    mov rsp, rbp
    pop rbp
    mov rax, 60
    mov rdi, 0
    syscall

