(define (prod [v (array-of Int)]) : Int
  (let ([p 1] [i 0])
    (while (< i (vector-length v))
      (set! p (* p (vector-ref v i)))
      (set! i (+ i 1)))
    p))
(let ([v (make-vector 5 0)])
  (vector-set! v 0 3)
  (vector-set! v 1 2)
  (vector-set! v 2 6)
  (vector-set! v 3 5)
  (vector-set! v 4 4)
  (print (prod v)))